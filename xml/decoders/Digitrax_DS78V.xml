          </variable>

          <variable CV="6.25" item="Servo 7 Thrown position" default="10">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="6.26" item="Servo 7 Closed position" default="240">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="6.27" item="Servo 7 'Approach' position" default="125">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="6.28" item="Servo 7 'Approach Medium' position" default="125">
            <decVal min="0" max="255" />
          </variable>

          <variable CV="7.25" item="Servo 8 Thrown position" default="10">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="7.26" item="Servo 8 Closed position" default="240">
