          </variable>

          <variable CV="3.25" item="Servo 4 Thrown position" default="10">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="3.26" item="Servo 4 Closed position" default="240">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="3.27" item="Servo 4 'Approach' position" default="125">
            <decVal min="0" max="255" />
          </variable>
          <variable CV="3.28" item="Servo 4 'Approach Medium' position" default="125">
            <decVal min="0" max="255" />
          </variable>

          <variable CV="4.25" item="Servo 5 Thrown position" default="10">
